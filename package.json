    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "eventemitter3": "^5.0.1",
    "expo": "~54.0.10",
    "expo-av": "~16.0.7",
    "expo-blur": "^15.0.7",
    "expo-build-properties": "^1.0.9",
