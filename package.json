{
  "name": "neurolearnapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "fix-android-dup": "node ./scripts/gradle-fix.js",
    "prebuild-android": "npx expo prebuild && node ./scripts/gradle-fix.js",
    "android-build": "cd android && .\\gradlew.bat clean assembleDebug",
    "eas-build-pre-install": "./eas-build-pre-install.sh",
    "build:ci": "npm run prebuild-android && cd android && ./gradlew assembleRelease",
    "verify-setup": "node scripts/verify-setup.js",
    "dev:full": "npm run setup-env && expo start",
    "build:local": "eas build --platform android --local",
    "test:all": "npm run validate-audio && npm test",
    "prebuild-check": "node scripts/pre-build-check.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --runInBand",
    "setup-env": "bash setup-env.sh",
    "setup-backend": "bash setup-env.sh && supabase functions deploy",
    "generate-audio": "node scripts/generate-audio-files.js",
    "validate-audio": "node scripts/validate-audio-assets.js",
    "generate-audio-guide": "node scripts/generate-audio-files.js",
    "validate-audio-assets": "node scripts/validate-audio-assets.js",
    "check-soundscapes": "node scripts/validate-audio-assets.js"
  },
  "dependencies": {
    "@miblanchard/react-native-slider": "^2.6.0",
