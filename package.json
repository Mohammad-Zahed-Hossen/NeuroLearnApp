    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@shopify/react-native-skia": "2.2.12",
    "@supabase/supabase-js": "^2.58.0",
    "@types/d3-force": "^3.0.10",
    "@types/d3-scale": "^4.0.9",
    "axios": "^1.12.2",
